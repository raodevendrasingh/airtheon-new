{
	"name": "react-router-cloudflare-saas-kit",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "npm run format && react-router build",
		"preview": "npm run build && wrangler dev",
		"deploy": "npm run build && wrangler deploy",
		"dev": "npm run format && react-router dev",
		"start": "wrangler dev",
		"typecheck": "react-router typegen && tsc -b",
		"format": "biome format --write .",
		"lint": "biome lint .",
		"check": "biome check --apply ."
	},
	"dependencies": {
		"@react-router/node": "^7.1.5",
		"@react-router/serve": "^7.1.5",
		"better-auth": "^1.1.17",
		"isbot": "^5.1.22",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-router": "^7.1.5"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@cloudflare/workers-types": "^4.20250204.0",
		"@react-router/dev": "^7.1.5",
		"@tailwindcss/vite": "^4.0.6",
		"@types/node": "^20.17.17",
		"@types/react": "^19.0.8",
		"@types/react-dom": "^19.0.3",
		"tailwindcss": "^4.0.6",
		"typescript": "^5.7.3",
		"vite": "^5.4.14",
		"vite-tsconfig-paths": "^5.1.4",
		"wrangler": "^3.107.3"
	}
}
